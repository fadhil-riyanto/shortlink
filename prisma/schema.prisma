generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Link {
    id          Int     @id @default(autoincrement())
    link        String  @db.Text
    conversion  String? @unique
    token       String? @unique
    clickcount  Int    @default(0)
}

